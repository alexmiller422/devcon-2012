<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>
    <!--  Initialise the DevCon content model. -->
    <bean id="devcon_dictionaryBootstrap" parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
        <property name="models">
            <list>
                <value>alfresco/extension/devconModel.xml</value>
            </list>
        </property>
        <property name="labels">
           <list>
              <!-- i18n strings for Model-->
              <value>alfresco/extension/devcon-model</value>
           </list>
        </property>
    </bean>
    
	<!-- Add java behaviours -->
	<bean id="assetBehaviours" class="org.alfresco.devcon2012.behaviours.AssetBehaviours" init-method="init">
		<property name="actionService" ref="ActionService"/>
		<property name="nodeService" ref="NodeService"/>
		<property name="policyComponent" ref="policyComponent"/>
		<property name="serviceRegistry" ref="ServiceRegistry"/>
		<property name="siteService" ref="SiteService"/>
	</bean>    
</beans>
