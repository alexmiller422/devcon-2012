<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

   <!-- Add Demo messages -->
   <bean id="webscripts.devcon.resources" class="org.springframework.extensions.surf.util.ResourceBundleBootstrapComponent">
      <property name="resourceBundles">
         <list>
            <value>alfresco.messages.devcon</value>
         </list>
      </property>
   </bean>

	<bean id="evaluator.isPurchaseOrder" parent="evaluator.doclib.action.hasAspect">
	  <property name="aspects">
	    <list>
	      <value>devcon:purchaseOrder</value>
	    </list>
	  </property>
	</bean>
</beans>
